<%= simple_form_for(@image_collection) do |f| %>
<%= f.error_notification %>

<div class="form-inputs">
  <%= f.input :name %>
  <%= f.input :attached_images, as: :select, collection: Image.all.order(id: :asc), input_html: {multiple: true} %>
</div>

<div class="form-actions">
  <%= f.button :submit %>
</div>
<% end %>
